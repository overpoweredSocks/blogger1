<h1><%= @article.title %></h1>
<p><%= @article.body %></p>
<%= link_to "Return to all articles", articles_path %>
<%= link_to "Edit", edit_article_path(@article) %>
<%= link_to "Delete", article_path(@article), method: :delete, 
 data: {confirm: "Are you sure you want to delete #{@article.title}?" } %>


